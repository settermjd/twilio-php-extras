{
  "name": "settermjd/twilio-php-extras",
  "description": "A small repository containing a series of extras and additions for Twilio's PHP Helper Library.",
  "type": "project",
  "license": "BSD-3-Clause",
  "keywords": [
    "PHP",
    "Twilio"
  ],
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^8.1 || ^8.2",
    "ext-json": "*",
    "twilio/sdk": "^7.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.3",
    "vlucas/phpdotenv": "^5.5"
  },
  "autoload": {
    "psr-4": {
      "Twilio\\": "src/Twilio/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TwilioTest\\": "test/Twilio/"
    }
  },
  "scripts": {
    "test": "phpunit --colors=always"
  }
}
